from openai import OpenAI

endpoint = "https://smvinu69-0911-resource.openai.azure.com/openai/v1/"
deployment_name = "gpt-oss-120b"
api_key = "ExifZF0uwyHNnguOppMLRhXj5YpByhc734KYeQGw6jIEJsK2m41RJQQJ99BKACF24PCXJ3w3AAAAACOGPPac"

client = OpenAI(
    base_url=endpoint,
    api_key=api_key
)

completion = client.chat.completions.create(
    model=deployment_name,
    messages=[
        {
            "role": "user",
            "content": "What is the capital of France?",
        }
    ],
    temperature=0.7,
)

print(completion.choices[0].message)